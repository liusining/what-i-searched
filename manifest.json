{
  "manifest_version": 2,
  "name": "What I Searched",
  "version": "0.3",
  "minimum_chrome_version": "55",
  "content_scripts": [{
    "matches": [
      "https://*/search*", "https://*.baidu.com/s*", "https://www.baidu.com/", "https://*/scholar*",
      "https://duckduckgo.com/*"
    ],
    "js": [
      "content/url.js"
    ]
  }],
  "permissions": [
    "storage"
  ],
  "browser_action": {
    "default_popup": "popup/popup.html"
  },
  "options_page": "opts/config.html",
  "key": "",
  "icons": {
    "16": "icons/what-i-searched-16.png",
    "48": "icons/what-i-searched-48.png",
    "128": "icons/what-i-searched-128.png"
  }
}